{
  "name": "iot-boilerplate-ctl",
  "version": "1.0.0",
  "license": "ISC",
  "description": "JS Front control for IoT based on ESP32",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RoiEf/IoT_Boiler_CTL.git"
  },
  "keywords": [
    "preact",
    "microbundle",
    "preact-router",
    "example",
    "SPA",
    "PWA"
  ],
  "source": "src/index.js",
  "scripts": {
    "build": "microbundle -f modern --no-sourcemap --external none -o build/bundle.js",
    "dev": "npm run -s build --watch & npm run -s serve",
    "serve": "sirv build -Dc"
  },
  "dependencies": {
    "preact": "^10.4.1",
    "preact-router": "^3.2.1"
  },
  "devDependencies": {
    "microbundle": "^0.12.0",
    "sirv-cli": "^0.4.5"
  }
}